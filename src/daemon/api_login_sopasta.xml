<service>
	<id>api_login_sopasta.exe</id>
	<name>API_LOGIN_SOPASTA</name>
	<description>Api para logins em apps da Sopasta</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Users\administrador.SOPASTA\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Apis\login\src\server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>API_LOGIN_SOPASTA wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>SVAPPS</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\Apis\login</workingdirectory>
</service>